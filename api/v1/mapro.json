{
  "openapi": "3.0.0",
  "info": {
    "title": "mapro",
    "description": "Maestros de Procesos",
    "version": "1.0.0"
  },
  "paths": {
    "/mapro/machine": {
      "get": {
        "summary": "Retrieves a Machine List",
        "responses": {
          "200": {
            "description": "Returns machine list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMachine"
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          }
        }
      }
    },
    "/mapro/job/{jobCode}": {
      "get": {
        "summary": "Get Job by Code",
        "responses": {
          "200": {
            "description": "Returns Job code",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Job Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "jobCode",
          "in": "path",
          "description": "Job Code",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ]
    },
    "/mapro/document": {
      "get": {
        "summary": "Retrieves a Document List",
        "parameters": [
          {
            "name": "job",
            "in": "query",
            "description": "Job Code",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "createdAt",
            "in": "query",
            "description": "Creation date",
            "schema": {
              "format": "date-time",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasDocument"
                }
              }
            }
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          }
        }
      }
    },
    "/mapro/standard": {
      "get": {
        "summary": "Retrieves a Standard List",
        "parameters": [
          {
            "name": "family",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "width",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sheet",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "machine",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasStandard"
                }
              }
            }
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          }
        }
      }
    },
    "/mapro/standard/{id}": {
      "get": {
        "summary": "Gets a Standard by ID",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasStandard"
                }
              }
            }
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasMaproError"
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ]
    }
  },
  "components": {
    "schemas": {
      "Machine": {
        "properties": {
          "machineType": {
            "$ref": "#/components/schemas/MachineType",
            "description": "Machine Type"
          },
          "name": {
            "description": "Machine Name",
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "description": "Creation date and time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "description": "Updated at date and time",
            "type": "string"
          },
          "deletedAt": {
            "format": "date-time",
            "description": "Deletion date and time",
            "type": "string"
          },
          "code": {
            "description": "Machine Code Identifier",
            "type": "string"
          },
          "machineComponents": {
            "description": "Machine Components",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MachineComponent"
            }
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "MachineType": {
        "properties": {
          "name": {
            "description": "Machine Type Name",
            "type": "string"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "MachineComponent": {
        "properties": {
          "name": {
            "description": "Component Name",
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "description": "Created at date and time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "description": "Updated at date and time",
            "type": "string"
          },
          "deletedAt": {
            "format": "date-time",
            "description": "Deleted at date and time",
            "type": "string"
          },
          "rangePercentage": {
            "format": "double",
            "description": "Range Percentage for Tolerance",
            "type": "number"
          },
          "children": {
            "description": "Children Components",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MachineComponent"
            }
          },
          "id": {
            "type": "integer"
          },
          "position": {
            "type": "integer"
          }
        }
      },
      "MeasurementUnit": {
        "properties": {
          "name": {
            "description": "Measurment Unit Name",
            "type": "string"
          },
          "symbol": {
            "description": "Measurement Unit Symbol",
            "type": "string"
          },
          "measurementType": {
            "$ref": "#/components/schemas/MeasurementType",
            "description": "Measurment Type"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "MeasurementType": {
        "properties": {
          "name": {
            "description": "Measurement type name",
            "type": "string"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "Document": {
        "properties": {
          "username": {
            "description": "Username",
            "type": "string"
          },
          "standard": {
            "$ref": "#/components/schemas/Standard",
            "description": "Standard"
          },
          "job": {
            "description": "Job",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "description": "Updated at date and time",
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "description": "Created at date and time",
            "type": "string"
          },
          "documentDetails": {
            "$ref": "#/components/schemas/DocumentDetail",
            "description": "Document Details"
          },
          "product": {
            "description": "Product",
            "type": "string"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "Standard": {
        "properties": {
          "machine": {
            "$ref": "#/components/schemas/Machine",
            "description": "Machine"
          },
          "username": {
            "description": "Username",
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "description": "Created at date and time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "description": "Updated at date and time",
            "type": "string"
          },
          "status": {
            "description": "Status",
            "type": "string"
          },
          "standardDetails": {
            "description": "Standard Details",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/StandardDetail"
            }
          },
          "width": {
            "description": "Width",
            "type": "string"
          },
          "client": {
            "description": "Client",
            "type": "string"
          },
          "rawMaterialA": {
            "description": "Raw Material A",
            "type": "string"
          },
          "rawMaterialB": {
            "description": "Raw Material B",
            "type": "string"
          },
          "sheet": {
            "type": "string"
          },
          "family": {
            "type": "string"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "StandardDetail": {
        "properties": {
          "machineComponent": {
            "$ref": "#/components/schemas/MachineComponent",
            "description": "Machine Component"
          },
          "minValue": {
            "format": "double",
            "description": "Minimum Value",
            "type": "number"
          },
          "stdValue": {
            "format": "double",
            "description": "Standard Value",
            "type": "number"
          },
          "maxValue": {
            "format": "double",
            "description": "Maximum Value",
            "type": "number"
          },
          "description": {
            "description": "Detail Description",
            "type": "string"
          },
          "detailType": {
            "description": "Detail Type in NUM, TXT,  MAT",
            "type": "string"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "DocumentDetail": {
        "properties": {
          "standardDetail": {
            "$ref": "#/components/schemas/StandardDetail",
            "description": "Standard Detail"
          },
          "description": {
            "description": "Document Detail Description",
            "type": "string"
          },
          "realValue": {
            "format": "double",
            "description": "Real Value",
            "type": "number"
          },
          "id": {
            "type": "integer"
          }
        }
      },
      "MaproError": {
        "properties": {
          "message": {
            "type": "string"
          },
          "element": {
            "type": "string"
          },
          "code": {
            "type": "string"
          }
        }
      }
    }
  }
}